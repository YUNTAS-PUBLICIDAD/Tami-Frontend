{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@icons/*":["src/assets/icons/*"],
      "@images/*":["src/assets/images/*"],
      "@data/*":["src/data/*"]
    }
  }
}